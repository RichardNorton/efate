{
  "name": "efate-graphql",
  "version": "0.1.0",
  "keywords": [
    "testing",
    "fixtures",
    "graphql"
  ],
  "author": "John Teague",
  "license": "MIT",
  "description": "generates fixture for graphql schemas, queries and mutations",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "precommit": "lint-staged",
    "test": "nyc --extension .ts mocha -r ts-node/register -r source-map-support/register src/**/*.tests.ts",
    "lint": "tslint ./src/**/*.ts",
    "prettier": "prettier  --parser typescript --single-quote",
    "prettier:write": "npm run prettier -- --write \"src/**/*.ts\""
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier:write",
      "npm run lint",
      "git diff --exit-code --stat"
    ]
  },
  "dependencies": {
    "efate": "^1.0.0",
    "efate-uuid": "^1.0.0",
    "graphql": "^14.5.3",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "husky": "^3.0.4",
    "lerna": "^3.16.4",
    "lint-staged": "^9.2.3",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}

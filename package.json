{
  "name": "efate",
  "version": "0.1.0",
  "description": "javascript test fixture generator.  Autofixturejs 2.0",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha -r ts-node/register src/**/*.tests.ts",
    "lint": "tslint ./src/**/*.ts",
    "prettier": "prettier  --parser typescript --single-quote",
    "prettier:write": "npm run prettier -- --write \"src/**/*.ts\""
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier:write",
      "npm run lint",
      "git diff --exit-code --stat"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcteague/efate.git"
  },
  "keywords": [
    "testing",
    "fixtures"
  ],
  "author": "John Teague",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcteague/efate/issues"
  },
  "homepage": "https://github.com/jcteague/efate#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/sinon-chai": "^3.2.0",
    "D": "^1.0.0",
    "ts-node": "^7.0.0"
  }
}
